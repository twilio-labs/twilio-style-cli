{
  "name": "twilio-style-cli",
  "version": "0.0.0",
  "description": "Twilio Style CLI",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/twilio-labs/twilio-style-cli",
  "bugs": "https://github.com/twilio-labs/twilio-style-cli/issues",
  "repository": "twilio-labs/twilio-style-cli",
  "license": "MIT",
  "author": "Alice Cai",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "twilio-style-cli": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "lint": "eslint --ext ts src/",
    "lint:fix": "npm run lint -- --fix",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "chai": "^4.3.4",
    "eslint": "^7.27.0",
    "eslint-config-twilio-ts": "^1.34.2",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "twilio-style-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
